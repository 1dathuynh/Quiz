{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "Database/db.json",
      "use": "@vercel/json-server"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/Database/db.json" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
